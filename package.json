  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@reduxjs/toolkit": "^1.3.6",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^11.3.1",
    "antd": "^4.3.3",
    "axios": "^0.19.2",
    "bignumber.js": "^9.0.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "crypto-js": "^4.0.0",
    "decimal.js": "^10.2.0",
    "faker": "^4.1.0",
    "history": "^4.10.1",
    "immer": "^7.0.5",
    "incognito-js": "https://github.com/incognitochain/sdk-v2",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.0",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-intl": "^4.6.9",
    "react-intl-redux": "^2.2.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-injectors": "^1.3.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "source-map-explorer": "^2.4.2",
    "styled-components": "^5.1.1",
    "use-immer": "^0.4.0"
  },
  "scripts": {
    "start": "cross-env REACT_APP_ENV=development react-scripts start",
    "staging": "cross-env REACT_APP_ENV=staging react-scripts start",
    "uat": "cross-env REACT_APP_ENV=uat react-scripts start",
    "prod": "cross-env REACT_APP_ENV=production GENERATE_SOURCEMAP=false react-scripts start",
    "build-dev": "cross-env REACT_APP_ENV=development react-scripts build",
    "build-staging": "cross-env REACT_APP_ENV=staging react-scripts build",
    "build-uat": "cross-env REACT_APP_ENV=uat react-scripts build",
    "build": "cross-env REACT_APP_ENV=production INLINE_RUNTIME_CHUNK=false react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "cross-env find . \\( -name node_modules -o -name __generated__ -o -name build \\) -prune -or \\( -name '*.js' -o -name '*.html' -o -name '*.scss' -o -name '*.json' -o -name '*.md' \\) -print | xargs prettier --write"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "webextensions": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-redux-saga": "^1.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "tabWidth": 4,
    "printWidth": 140
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,scss,md}": [
      "prettier --write"
    ]
  }
}
